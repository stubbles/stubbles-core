{
    "name": "stubbles/core",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Provides base classes for Stubbles packages.",
    "repositories": [
       {
           "type": "vcs",
           "url": "git@github.com:stubbles/stubbles-streams.git"
       },
       {
           "type": "vcs",
           "url": "git@github.com:stubbles/stubbles-reflect.git"
       }
    ],
    "require": {
        "php": ">=5.6.0",
        "stubbles/sequence": "^7.0",
        "stubbles/values": "^7.0",
        "stubbles/reflect": "^7.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^5.1",
        "bovigo/callmap": "^2.0",
        "bovigo/assert": "^1.5",
        "stubbles/streams": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\": "src/main/php"
        },
        "files": ["src/main/php/lang/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\test\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
